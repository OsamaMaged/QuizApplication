<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.quiz">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/quiz"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.Quiz">
        <activity android:name=".views.AdminOptionsActivity"></activity>
        <activity android:name=".views.DeletingQuiz" />
        <activity android:name=".views.AddingQuestions" />
        <activity android:name=".views.AddingQuiz" />
        <activity android:name=".views.ResultActivity" />
        <activity android:name=".views.QuizActivity" />
        <activity android:name=".views.detailsActivity" />
        <activity android:name=".views.ListActivity" />
        <activity android:name=".views.adminActivity" />
        <activity android:name=".views.userActivity" />
        <activity android:name=".views.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>